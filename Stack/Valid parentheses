import java.util.Stack;
class Solution {
    public boolean isValid(String s) {
        if(s.length()==0){
            return true;
        }
        if(s.length()%2!=0){
            return false;
        }
        Stack<Character> st=new Stack<>();
        for(int i=0;i<s.length();i++){
            char ch=s.charAt(i);
            char c;
            if(ch=='{'||ch=='['||ch=='('){
                st.push(ch);
            }
            
            else if(st.empty()&&(ch=='}'||ch==']'||ch==')')){
                return false;
            }
                    
            else if(!st.empty()){
             if(ch=='}' && st.peek()=='{'){
                 c=st.pop();
            }
            else if(ch==']' && st.peek()=='['){
                 c=st.pop();
            }
            else if(ch==')' && st.peek()=='('){
                 c=st.pop();
            }
            else{
                return false;
            }
            }
        }
        
        if(st.empty()){
            return true;
        }
        return false;
    }
}
